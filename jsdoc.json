{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": "./core",
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs|logs/|plugins/)"
    },
    "plugins": [

    ],
    "opts": {
        "template": "node_modules/minami",
        "encoding": "utf8",
        "destination": "docs/build",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
